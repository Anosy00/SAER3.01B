ip address add 172.1.1.3/29 dev eth0
set dev eth0 up
ip address add 192.168.31.254/20 dev eth1
set dev eth1 up
echo "
nameserver 8.8.8.8
search www.google.com" > /etc/resolv.conf
apt update -y && apt upgrade -y
iptables -P FORWARD DROP
iptables -A FORWARD -p tcp --sport 587 -s 192.168.23.1 -d 192.168.16.0/20 -j ACCEPT
iptables -A FORWARD -i eth1 -p tcp --dport 587 -d 192.168.23.1 -j ACCEPT
iptables -A FORWARD -p tcp --sport 22 -s 172.12.150.1  -j ACCEPT
